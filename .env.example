# Environment Variables for Drip IV Dashboard
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/drip_iv_dashboard

# Server Configuration
NODE_ENV=production
PORT=3000

# Security (Optional - generated automatically if not provided)
SESSION_SECRET=your-super-secret-session-key-here

# File Upload Settings
MAX_FILE_SIZE=10485760
UPLOAD_PATH=uploads/

# Application Settings
APP_NAME=Drip IV Dashboard
APP_VERSION=1.0.0

# Railway Database (Railway will auto-populate this)
# DATABASE_URL will be automatically set by Railway when you provision PostgreSQL
